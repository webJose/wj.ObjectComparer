<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>webJose (webJose@gmail.com)</Authors>
    <Company />
    <Description>Class library that compares the values of properties from different objects in order to evaluate equality.</Description>
    <PackageProjectUrl>https://github.com/webJose/wj.ObjectComparer</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/webJose/wj.ObjectComparer/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/webJose/wj.ObjectComparer</RepositoryUrl>
    <PackageTags>compare comparer objectcomparer property comparison</PackageTags>
    <NeutralLanguage>en-US</NeutralLanguage>
    <LangVersion>latest</LangVersion>
    <Version>1.0.1</Version>
    <AssemblyVersion>1.0.1</AssemblyVersion>
    <FileVersion>1.0.1</FileVersion>
    <PackageReleaseNotes>Version 1.0.0 is the first production grade version.</PackageReleaseNotes>
    <PackageIconUrl>https://github.com/webJose/wj.ObjectComparer/blob/master/Logo_1024.png</PackageIconUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net461'">
    <Reference Include="System.ComponentModel.DataAnnotations" />
  </ItemGroup>
</Project>
